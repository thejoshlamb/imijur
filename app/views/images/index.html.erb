<div class = 'grid'>
	<% @most_recent_six.each do |image| %>
		<div class = "big_imgbox"><%= link_to image_tag(image.URL), "/images/#{image.id}" %></div>
	<% end %>
</div>
<div class = 'grid'>
	<% @images.each do |image| %>
		<div class = "imgbox"><%= link_to image_tag(image.URL), "/images/#{image.id}" %></div>
	<% end %>
</div>

<div class = "new">
	<%= render partial: "form" %>
</div>

<div id = "footer">
	images over 1 month old: <%= Image.created_before(1.month.ago).count %>
</div>
